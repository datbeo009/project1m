    <script>
        function LoadCategory() {
            $('#productSubmenu').html('');
            $.ajax({
                url: "/ClientCategory/Gets",
                type: "Get",
                success: function (res) {
                    res = JSON.parse(res);
                    if (res) {
                        res.forEach(e => {
                            var CategoryName = e.CategoryName;
                            $('#productSubmenu').append("<li><a style='font-size:20px' href='/ClientProduct/Index?cate=" + e.CategoryID + "'>" + CategoryName + "</a></li>");
                            $('.cat-list').append('<li><a href="/ClientProduct/Index?cate=' + e.CategoryID + '" class="d-flex"><p>'+e.CategoryName+'</p></a></li>')
                        })
                    }
                }

            })
        }

        function loadCartCount() {
            $.ajax({
                url: '/ShoppingCart/CartCount',
                type: 'get',
                success: function (res) {
                    if (res) {
                        $('#spCart').attr('data-content',res)
                    }
                }
            })
        }

        function checkLogin() {
            $.ajax({
                url: '/ClientLogin/CheckLogInfo',
                type: 'get',
                success: function (res) {
                    var obj = JSON.parse(res);
                    if (obj == null) {
                        $('#dvLogin').show();
                        $('#dvUser').hide();
                    } else {
                        $('#dvLogin').hide();
                        $('#dvUser').show();
                        $('#userName').html(obj.UserName);
                    }
                }
            })
        }

        $('#lgOut').on('click', function () {
            $.ajax({
                url: '/ClientLogin/LogOut',
                type: 'get',
                success: function () {
                    $('#dvLogin').show();
                    $('#dvUser').hide();
                }
            })
            return false;
        })

        loadCartCount();
        LoadCategory();
        checkLogin();
        

    </script>

